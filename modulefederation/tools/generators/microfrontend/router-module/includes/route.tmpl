 {
  <% if (feature.module && !isChild) { %>
      path: '<%- feature.router?.path || feature.id -%>',
      loadChildren: () => import('<%= feature.module.relative %><%= feature.module.file.file.substring(0, feature.module.file.file.indexOf(".")) %>').then(m => m.<%= feature.module.name %> )
  <% } else { %>
         path: '<%- isChild ? "" : feature.id -%>',
         component: <%= feature.component %>,
         children: [
           <% for (let child of (feature.children || [])) { %>
             <%- include(process.cwd() + '/tools/generators/microfrontend/router-module/includes/route.tmpl', { feature: child }) -%>
           <% } %>
         ]
   <% } %>
  },
